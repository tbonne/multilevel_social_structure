package multiLevelGroups;

public class Runnable_OMUs implements Runnable  {
	
OMU omu;
	
	Runnable_OMUs(OMU o){
		omu = o;
	}
	
	@Override
	public void run(){
		Throwable thrown = null;
	    try {
		omu.decision();
	    } catch (Throwable e) {
	        thrown = e;
	        System.out.println("Problem lies in OMU code  " + thrown);
	        
	    } finally {
	    	return;
	    }
	}
	    

}
